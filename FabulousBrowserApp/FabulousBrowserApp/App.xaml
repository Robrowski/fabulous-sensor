<Application
    x:Class="FabulousBrowserApp.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FabulousBrowserApp">
    
    <Application.Resources>
        <SolidColorBrush x:Key="ListBoxBackgroundThemeBrush" Color="#FF000000" />
        <SolidColorBrush x:Key="ListBoxBorderThemeBrush" Color="#FF000000" />
        <SolidColorBrush x:Key="ListBoxDisabledForegroundThemeBrush" Color="#FF000000" />
        <SolidColorBrush x:Key="ListBoxFocusBackgroundThemeBrush" Color="#FF000000" />
        <SolidColorBrush x:Key="ListBoxForegroundThemeBrush" Color="#FF000000" />
    </Application.Resources>

</Application>
